    </script><script src="https://cdn.pagefly.io/pagefly/3.25.12/core/helper.js" async></script><script>const url = 'https://buy.thefemalecompany.com/collections/';
const filterDataBtn = document.querySelectorAll('.filterDataBtn');
const mobileSelect = document.querySelectorAll('.mobileFilter');

filterDataBtn.forEach(function(element){
	element.addEventListener('click', function(e) {  
		
		document.querySelectorAll('.filterDataBtn').forEach(function(item){
			if(item.dataset.value !== e.target.dataset.value) {
				if(item.dataset.type === e.target.dataset.type) {
					item.classList.remove('selected');
				}	
			}
		});
		
		e.target.classList.toggle("selected");
		
		getProducts('desktop');
	})
});

mobileSelect.forEach(function(element){
	element.addEventListener('change', function(e) { 
		getProducts('mobile');
	})
});

function getProducts(device = '') {
	const filterQuery = getSelectedFilters(device).split('?')[0] !== '' ? getSelectedFilters(device) : `all${getSelectedFilters(device)}`;
	
	fetch(`${url}${filterQuery}`, { method: 'GET' })
	.then(response => response.text())
	.then(html => {
	    const responseDocument = new DOMParser().parseFromString(html, 'text/html');
	    const resContainer = responseDocument.getElementById('facet-main');
		document.getElementById('filter-resutls').innerHTML = '';
		document.getElementById('filter-resutls').insertAdjacentHTML('beforeend', resContainer.innerHTML);
		showHideDiscountProducts();
	});	
}

function getSelectedFilters(device) {
	let qureyString = '';
	
  if(device === 'mobile') {
	document.querySelectorAll('.mobileFilter').forEach(function(item){		
		if(item.dataset.type === 'productType') {
			qureyString += `${item.value}`;
		}
		
		if(item.dataset.type === 'productOption') {
			if(item.value !== '') {
				qureyString += `?&filter.v.option.größe=${item.value}`;
			}
		}
	});
	
  } else {
	document.querySelectorAll('.filterDataBtn.selected').forEach(function(item){		
		if(item.dataset.type === 'productType') {
			qureyString += `${item.dataset.value}`;
		}
		
		if(item.dataset.type === 'productOption') {
			qureyString += `?&filter.v.option.größe=${item.dataset.value}`;
		}
	});	
  } 
return qureyString;
}


function showHideDiscountProducts() {
	console.log('Discount');
	const selectedDiscount = document.querySelector('.ftsa-item.selected');
	const selectedDiscountMobile = document.querySelector('select[name="productSale"]');
	
	const selectedDiscountAmount = selectedDiscount ? selectedDiscount.dataset.value : selectedDiscountMobile.value;
	
	if(selectedDiscount || selectedDiscountMobile && selectedDiscountAmount !== '') {
		
		
		document.querySelectorAll('.product-list__inner product-item.product-item').forEach(function(item, index) {
			const productDiscountLabel = item.querySelector('.product-item__image-wrapper .product-item__label-list .label--highlight');
			
			if(productDiscountLabel) {
				const productDiscount = productDiscountLabel.textContent.split('Spare')[1].trim().slice(0,-1);
				if(productDiscount !== selectedDiscountAmount) {
					item.remove();
				}	
			} else {
				item.remove();
			}
		});
		
		if(!document.querySelectorAll('.product-list__inner product-item.product-item').length) {
			const noProducts = `<div class="empty-state"><h3 class="heading h4">Keine Ergebnisse</h3>
		      <p>Entschuldige, Deine Suche führte zu keinen Ergebnissen.</p>
		
		      <div class="button-wrapper">
		        <a href="/collections/merch?sort_by=manual" data-action="clear-filters" class="button button--primary">Filter zurücksetzen</a>
		      </div></div>`;
		      
		  if(!document.querySelector('.empty-state')) {
			document.getElementById('filter-resutls').insertAdjacentHTML('beforeend', noProducts);
}
			
		}
	}
	
}

getProducts();


document.querySelectorAll('.deal-tab').forEach(function(el,i) {
  el.addEventListener('click', function(e) {
	e.stopPropagation();
	console.log(e.target.classList);
	if(e.target.classList.contains('single') || e.target.parentElement.classList.contains('single') ||
	  e.target.offsetParent.classList.contains('single')) {
	  document.getElementById('multi').style.display='none';
	  document.getElementById('single').style.display='block';	  
	} else if(e.target.classList.contains('multi') || e.target.parentElement.classList.contains('multi') ||
			  e.target.offsetParent.classList.contains('multi')) {
	  document.getElementById('single').style.display='none';
	  document.getElementById('multi').style.display='block';	
	}
  })
});

document.getElementById('multi').style.display='none';</script>
